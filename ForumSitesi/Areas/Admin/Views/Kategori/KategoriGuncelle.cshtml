@model Category
<ol class="breadcrumb mb-4">
    <li<h1 class="mt-4"> Kategori Güncelle
    </h1>
        <div class="breadcrumb-item active">Admin</li>
</ol>
<div class="row">
    <div class="inline">
        <form method="post" action="KategoriGuncelle">
            <label class="text">Kategori Adı </label>
            @Html.HiddenFor(x => x.CategoryID)
            @Html.TextBoxFor(x => x.CategoryName, new { @class = "form-control" })
            <button class="btn btn-primary">Güncelle</button>
        </form>
    </div>
</div>

